Sub CargaDatosGrabadosConfiguracionUsuario As Boolean
	Dim ConfUsuario As Boolean=True
	Try
		If File.Exists(UserFolder, "ConfiguracionUsuario.txt") Then
			Dim ser As B4XSerializator
			Dim opt() As Object = ser.ConvertBytesToObject(File.ReadBytes(UserFolder, "ConfiguracionUsuario.txt"))
			mConfiguracionUsuario = opt(0)
		End If
	Catch
		Log(LastException)
		
		ConfUsuario=False
	End Try
	Return ConfUsuario
End Sub

