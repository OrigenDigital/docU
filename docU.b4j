	Wait For DatosJRDC_Completed (mresult As Map)
	If mresult.Get("ConnOK")=False Then
'		Dim msa As Object=xui.MsgboxAsync("Error de conexion servidor/query","Error")
'		Wait For (msa) Msgbox_Result
'		frm.Close
		Accion="ErrConn"
