'Return true to allow the default exceptions handler to handle the uncaught exception.
Sub Application_Error (Error As Exception, StackTrace As String) As Boolean
	'Return True
	
	LogError(StackTrace)
	' borrar el fichero, si existe
	If File.Exists(xui.DefaultFolder,"logCrash.txt") Then
		File.Delete(xui.DefaultFolder,"logCrash.txt")
	End If
	DateTime.DateFormat="dd/MM/yy hh:mm:ss"
	Dim sb As StringBuilder
	sb.Initialize
	sb.Append("Error " & DateTime.Date(DateTime.Now)).Append(CRLF).Append(CRLF).Append(StackTrace)
	sb.Append(CRLF).Append(Error)
	Dim sErrMsg As String=sb.ToString
	' grabar el fichero con el error, para ser enviado por email la siguiente vez que este usuario abra la aplicacion
	File.WriteString(xui.DefaultFolder,"logCrash.txt",sErrMsg)
	EnviarEmailErrorApp
	Return False
End Sub

Sub EnviarEmailErrorApp
	Dim SMTP As SMTP
	SMTP.Initialize(DatosConfiguracionAplicacion.ServidorEnvioEmail, DatosConfiguracionAplicacion.PuertoEnvioEmail, _
			 DatosConfiguracionAplicacion.DireccionEnvioEmail, DatosConfiguracionAplicacion.PasswordEnvioEmail, "SMTP")
	SMTP.StartTLSMode = True
	SMTP.UseSSL = True 'Gmail requires SSL.
	SMTP.AuthMethod = SMTP.AUTH_LOGIN
	If DatosConfiguracionAplicacion.EmailDesarrollador1<>"" Or DatosConfiguracionAplicacion.EmailDesarrollador2<>"" Then
		If DatosConfiguracionAplicacion.EmailDesarrollador1<>"" Then
			SMTP.To.Add(DatosConfiguracionAplicacion.EmailDesarrollador1)
		End If
			
		If DatosConfiguracionAplicacion.EmailDesarrollador2<>"" Then
			SMTP.To.Add(DatosConfiguracionAplicacion.EmailDesarrollador2)
		End If
			
		SMTP.Subject = "App crash " & NombreAplicacion
			
		'SMTP.Body = "Usuario ejecutando la aplicación cuando ocurre el error: " & DatosUsuario.NombreUsuario
		SMTP.Body = "Error en versión " & Version & CRLF & DatosUsuario.Dispositivo & "   " & DatosUsuario.NombreUsuarioWindows
			
		SMTP.AddAttachment(xui.DefaultFolder,"logCrash.txt")
		
		Dim SendEmail As Object =SMTP.Send
		Wait For (SendEmail) SMTP_MessageSent(Success As Boolean)
		If Success Then
			File.Delete(xui.DefaultFolder,"logCrash.txt")
			ExitApplication
		Else
			Log("Error enviando fichero appCrash por email")
			ExitApplication
		End If
	End If
	
End Sub




Sub CargaPermisosUsuario As ResumableSub
	Dim PermisoAPP As String
	Wait For(cmPermisos.PermisosAplicacionUsuarioObjetoPermiso(rdclinkMySqlDatosComunes, "PermisosAplicacion",NombreAplicacion,DatosUsuario.NombreUsuarioWindows,"Aplicacion")) complete (sRes As String)
	Select Case sRes
		Case "ErrConn"
			Dim msa As Object=xui.MsgboxAsync("Error de conexion servidor/query.","Error")
			Wait For (msa) Msgbox_Result
			PermisoAPP="NO"
		Case "SinPermiso"
			'en esta aplicacion (docU), no hace falta tener permiso explicito para entrar
'			Dim msa As Object=xui.MsgboxAsync("No tienes permiso para acceder a esta aplicación.","Aviso")
'			Wait For (msa) Msgbox_Result
			'PermisoAPP="NO"
			PermisoAPP="OK"
		Case "ERROR"
			Dim msa As Object=xui.MsgboxAsync(sRes,"Error")
			Wait For (msa) Msgbox_Result
			PermisoAPP="NO"
		Case "DENIED"
			Dim msa As Object=xui.MsgboxAsync("No tienes permiso para acceder a  esta aplicación.","Aviso")
			Wait For (msa) Msgbox_Result
			PermisoAPP="NO"
		Case Else
			'Dim PermisoModuloUsuario As String=sRes
			PermisoAPP=sRes
	End Select
	Return PermisoAPP
End Sub

